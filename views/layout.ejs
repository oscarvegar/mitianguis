    <style type="text/css">


div.overlay {
    display:        table;
    position:       fixed;
    top:            0;
    left:           0;
    width:          100%;
    height:         100%;
}
div.overlay > div {
    display:        table-cell;
    width:          100%;
    height:         100%;
    background:     #ccc;
    text-align:     center;
    vertical-align: middle;
}
    </style>
     <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <META HTTP-EQUIV="Pragma" CONTENT="no-cache">
    <META HTTP-EQUIV="Expires" CONTENT="-1">
    <meta property="og:type" content="website" />

    <!-- si viene un producto -->
    <% if(typeof producto != 'undefined'){%>
    <meta property="og:image" itemprop="image primaryImageOfPage" content="<%=producto?producto.imagenPrincipal:null%>" />
    <meta name="twitter:title" id="mtt" property="og:title" itemprop="title name" content="<%=producto.nombre%> a <%= producto.precioFormat%>" />
    <meta name="twitter:description" property="og:description" itemprop="description" content="<%=producto.descripcionCorta%>" />
    <meta name="twitter:card" content="photo" />
    <meta name="twitter:site" content="@mMiTianguisMX" />
    <meta name="twitter:title" content="<%=producto.nombre%> a <%= producto.precioFormat%>" />
    <meta name="twitter:image" content="<%=producto.imagenPrincipal%>" />
    <meta name="twitter:url" content="http://mitianguis.mx/<%=redirectURL%>" />

    <%}%>
    <!-- si viene un blog -->
    <% if(typeof blog != 'undefined'){%>
      <meta property="og:image" itemprop="image primaryImageOfPage" content="/getImagenBlog/<%=blog.imagen%>" />
      <meta name="og:title" id="mtt" property="og:title" itemprop="title name" content="<%=blog.titulo%>" />
      <meta name="og:description" property="og:description" itemprop="description" content="<%=blog.resumen%>" />
      <meta name="HandheldFriendly" content="True"><title><%=blog.titulo%></title>    
        <meta name="description" content="<%=blog.resumen%>">

    <%}%>
